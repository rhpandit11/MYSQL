    "select\n",
    "*\n",
    "from useractivity"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "vscode": {
     "languageId": "sql"
    }
   },
   "outputs": [],
   "source": [
    "with cte as\n",
    "(\n",
    "select *,\n",
    "count(1) OVER(PARTITION BY username) as total,\n",
    "ROW_NUMBER() over(PARTITION BY username ORDER BY startdate) as rnk\n",
    "from useractivity\n",
    ")\n",
    "select * from cte\n",
    "where total = 1 or rnk = 2"
   ]
  }
